{
  "name": "@edx/frontend-template-application",
  "version": "0.1.0",
  "description": "Frontend application template",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/openedx/frontend-template-application.git"
  },
  "browserslist": [
    "last 2 versions",
    "ie 11"
  ],
  "scripts": {
    "build": "fedx-scripts webpack",
    "i18n_extract": "BABEL_ENV=i18n fedx-scripts babel src --quiet > /dev/null",
    "is-es5": "es-check es5 ./dist/*.js",
    "lint": "fedx-scripts eslint --ext .js --ext .jsx .",
    "lint:fix": "fedx-scripts eslint --fix --ext .js --ext .jsx .",
    "snapshot": "fedx-scripts jest --updateSnapshot",
    "start": "fedx-scripts webpack-dev-server --progress",
    "test": "fedx-scripts jest --coverage --passWithNoTests"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "author": "edX",
  "license": "AGPL-3.0",
  "homepage": "https://github.com/openedx/frontend-template-application#readme",
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/openedx/frontend-template-application/issues"
  },
  "dependencies": {
    "@edx/brand": "https://github.com/Edspirit/brand-openedx",
    "@edx/frontend-platform": "4.2.0",
    "@edx/paragon": "20.28.5",
    "@reduxjs/toolkit": "^1.9.5",
    "axios": "^0.21.4",
    "axios-cache-adapter": "^2.7.3",
    "classnames": "2.2.6",
    "core-js": "3.25.5",
    "lodash.escaperegexp": "4.1.2",
    "prop-types": "15.8.1",
    "react": "17.0.0",
    "react-dom": "17.0.0",
    "react-helmet": "^6.1.0",
    "react-intersection-observer": "^9.4.3",
    "react-query": "^3.39.3",
    "react-redux": "7.2.9",
    "react-router": "5.3.4",
    "react-router-dom": "5.3.4",
    "react-scroll": "^1.8.9",
    "react-scrollspy": "^3.4.3",
    "react-show-more-text": "^1.6.2",
    "redux": "4.2.0",
    "regenerator-runtime": "0.13.10",
    "video-react": "^0.16.0"
  },
  "devDependencies": {
    "@edx/browserslist-config": "^1.1.1",
    "@edx/frontend-build": "^12.9.0-alpha.1",
    "@edx/reactifex": "^2.1.1",
    "@edx/typescript-config": "^1.0.1",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "es-check": "5.2.4",
    "glob": "7.2.3",
    "husky": "7.0.4",
    "jest": "29.4.3",
    "reactifex": "1.1.1",
    "typescript": "^4.9.5"
  }
}
